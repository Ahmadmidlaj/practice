//skip any particualr character in a string



class HelloWorld {
    public static void main(String[] args) {
        String s ="ahmada";
        char skipchar='a';
        String result=skip(s,skipchar);
        System.out.println(result);
    }
    public static String skip(String s,char skipchar){
        String result=" ";
        for(int i=0;i<s.length();i++){
            char currentchar=s.charAt(i);
            
            if(currentchar!=skipchar){
                result+=currentchar;
            }
         
            
            
            
        } 
           return result;
    }
}
    


